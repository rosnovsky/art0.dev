{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "art0.dev#build": {
      "dependsOn": [
        "^prismadb#build"
      ],
      "env": [
        "NEXT_PUBLIC_VERCEL_URL",
        "PORT",
        "NODE_ENV",
        "RENDER_INTERNAL_HOSTNAME"
      ]
    },
    "prismadb#build": {
      "env": [
        "DATABASE_URL"
      ]
    },
    "dev": {
      "dependsOn": [
        "prismadb#build"
      ],
      "cache": false
    }
  }
}
