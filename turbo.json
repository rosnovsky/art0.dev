{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
	"build": {
		"dependsOn":["^build"]
},
    "web#build": {
      "dependsOn": ["^build"],
      "env": ["NEXT_PUBLIC_VERCEL_URL", "PORT", "NODE_ENV", "RENDER_INTERNAL_HOSTNAME"]
    },
    "dev": {
      "cache": false
    }
  },
"globalEnv": [
    // "VERCEL_URL", // contents will impact hashes of all tasks
//	"NEXT_PUBLIC_PORT", "NODE_ENV", "RENDER_INTERNAL_HOSTNAME"
     // contents will impact hashes of all tasks
  ]
}
