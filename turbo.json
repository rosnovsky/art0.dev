{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^lint",
        "^build"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "prismadb#build": {
      "env": [
        "DATABASE_URL"
      ]
    },
    "dev": {
      "dependsOn": [
        "prismadb#build"
      ]
    }
  },
  "globalEnv": [
    "VERCEL_URL",
    "NEXT_PUBLIC_VERCEL_URL",
    "PORT",
    "NODE_ENV",
    "RENDER_INTERNAL_HOSTNAME",
    "DATABASE_URL"
  ]
}
